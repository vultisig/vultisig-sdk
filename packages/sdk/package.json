{
  "name": "@vultisig/sdk",
  "version": "0.1.0-beta.1",
  "description": "TypeScript SDK for secure multi-party computation and blockchain operations",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "lib",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./node": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.node.js"
    },
    "./wasm": {
      "types": "./dist/wasm/index.d.ts",
      "import": "./dist/wasm/index.esm.js",
      "require": "./dist/wasm/index.js"
    }
  },
  "scripts": {
    "build": "yarn build:js && yarn build:types",
    "build:js": "node ../../node_modules/.bin/rollup -c rollup.config.js",
    "build:types": "tsc -p tsconfig.declarations.json",
    "build:debug": "node --max-old-space-size=16384 --trace-warnings --expose-gc --trace-gc ../../node_modules/.bin/rollup -c rollup.config.js --perf",
    "dev": "rollup -c rollup.config.js -w",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run --config tests/integration/vitest.config.ts tests/integration",
    "test:e2e": "yarn test:e2e:balance; yarn test:e2e:gas; yarn test:e2e:multi-chain; yarn test:e2e:prepare-send",
    "test:e2e:balance": "vitest run --config tests/e2e/vitest.config.ts tests/e2e/balance-operations.test.ts",
    "test:e2e:gas": "vitest run --config tests/e2e/vitest.config.ts tests/e2e/gas-estimation.test.ts",
    "test:e2e:multi-chain": "vitest run --config tests/e2e/vitest.config.ts tests/e2e/multi-chain-coverage.test.ts",
    "test:e2e:prepare-send": "vitest run --config tests/e2e/vitest.config.ts tests/e2e/prepare-send-tx.test.ts",
    "test:watch": "vitest",
    "test:unit:watch": "vitest tests/unit",
    "test:e2e:watch": "vitest --config tests/e2e/vitest.config.ts tests/e2e",
    "test:changed": "vitest related --run",
    "lint": "eslint --config ../../.config/eslint.config.mjs src/**/*.{ts,tsx}",
    "lint:fix": "eslint --config ../../.config/eslint.config.mjs src/**/*.{ts,tsx} --fix",
    "prepublishOnly": "npm run typecheck",
    "prepack": "echo 'Build skipped for development'"
  },
  "keywords": [
    "vultisig",
    "multi-party-computation",
    "mpc",
    "blockchain",
    "cryptocurrency",
    "wallet",
    "security",
    "threshold-signatures",
    "web3"
  ],
  "author": {
    "name": "Vultisig",
    "email": "info@vultisig.com",
    "url": "https://vultisig.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vultisig/vultisig-sdk.git",
    "directory": "vultisig-sdk"
  },
  "bugs": {
    "url": "https://github.com/vultisig/vultisig-sdk/issues"
  },
  "homepage": "https://vultisig.com",
  "dependencies": {
    "7z-wasm": "^1.2.0",
    "@bufbuild/protobuf": "^2.7.0",
    "@coral-xyz/anchor": "^0.28.0",
    "@cosmjs/stargate": "^0.36.0",
    "@lifi/sdk": "^3.12.2",
    "@mysten/sui": "^1.37.6",
    "@noble/hashes": "^2.0.0",
    "@polkadot/api": "^16.4.6",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^2.0.0",
    "@trustwallet/wallet-core": "^4.3.6",
    "axios": "^1.8.3",
    "bip32": "^5.0.0-rc.0",
    "bitcoinjs-lib": "^6.1.7",
    "bs58": "^6.0.0",
    "cbor-x": "^1.6.0",
    "date-fns": "^4.1.0",
    "ethers": "^6.13.5",
    "i18next": "^25.5.2",
    "i18next-http-backend": "^3.0.2",
    "node-fetch": "^3.3.2",
    "tiny-secp256k1": "^2.2.4",
    "uuid": "^11.1.0",
    "viem": "2.21.0",
    "xrpl": "^4.4.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/chrome": "^0.1.27",
    "@types/node": "^22.13.10",
    "@vitest/coverage-v8": "3.2.4",
    "dotenv": "^17.2.3",
    "rollup": "^4.32.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.1.1",
    "tsx": "^4.19.5",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
