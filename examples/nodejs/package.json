{
  "name": "@vultisig/example-nodejs",
  "version": "1.0.0",
  "description": "Complete CLI wallet example demonstrating the Vultisig SDK for Node.js",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "wallet": "tsx src/index.ts",
    "typecheck": "tsc --noEmit"
