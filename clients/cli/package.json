{
  "name": "@vultisig/cli",
  "version": "0.1.0-beta.1",
  "description": "Command-line wallet for Vultisig - multi-chain MPC wallet management",
  "type": "module",
  "bin": {
    "vultisig": "./dist/index.js",
    "vsig": "./dist/index.js"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "wallet": "tsx src/index.ts",
    "wallet:interactive": "tsx src/index.ts --interactive",
    "repl": "tsx src/index.ts --interactive",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vultisig",
    "wallet",
    "cli",
    "blockchain",
    "crypto",
    "mpc",
    "multi-party-computation"
  ],
  "author": {
    "name": "Vultisig",
    "email": "info@vultisig.com",
    "url": "https://vultisig.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vultisig/vultisig-sdk.git",
    "directory": "clients/cli"
  },
  "bugs": {
    "url": "https://github.com/vultisig/vultisig-sdk/issues"
  },
  "homepage": "https://vultisig.com",
  "dependencies": {
    "@vultisig/sdk": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.0",
    "inquirer": "^9.2.0",
    "ora": "^8.0.0",
    "tabtab": "^3.0.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.0",
    "@types/node": "^22.13.10",
    "@types/tabtab": "^3.0.4",
    "@types/ws": "^8.18.1",
    "tsx": "^4.7.0",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
