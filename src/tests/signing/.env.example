# Vultisig CLI Environment Variables
# Copy this file to .env and update with your values

# Path to your vault file (.vult)
# Can be absolute path or relative to the CLI directory
VAULT_PATH=vaults/HotVault.vult

# Password for encrypted vault files
# Leave empty if your vault is unencrypted
VAULT_PASSWORD=

# Optional: Default network for signing operations
# Supported: eth, btc, sol, ltc, doge (short names) or ethereum, bitcoin, solana, litecoin, dogecoin (full names)
DEFAULT_NETWORK=eth

# Optional: Default signing mode
# Supported: local, relay, fast
DEFAULT_SIGNING_MODE=fast
